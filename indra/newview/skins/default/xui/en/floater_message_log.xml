		<scroll_list.columns name="icon0" label="icon0" width="24" />
		<scroll_list.columns name="icon1" label="icon1" width="24" />
	</scroll_list>
	<button name="filter_choice_btn" follows="left|top" left="10" top="149" right="30" bottom="169" label="…"/>
	<line_editor name="filter_edit" follows="left|top|right" left="30" top="149" right="342" bottom="169" max_length="65535"/>
