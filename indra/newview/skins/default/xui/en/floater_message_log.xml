	</scroll_list>
	<text name="log_status_text" follows="left|top|right" left="10" top="-172" right="-10" height="20">
	</text>
	<text_editor name="net_info" follows="left|top|right|bottom" left="10" top="448" bottom="560" right="-10" enabled="false" max_length="65535">
	</text_editor>
	<button name="send_to_message_builder_btn" follows="right|bottom" left="210" top="570" right="390" bottom="590" label="Send to Message Builder" enabled="false"/>
</floater>
